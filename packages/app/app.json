{
  "expo": {
    "name": "Recipes",
    "icon": "./src/assets/temp_icon.png",
    "version": "1.0.0",
    "slug": "Recipes",
    "orientation": "portrait",
    "splash": {
      "image": "./src/assets/temp_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/temp-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.recipes"
    },
    "web": {
      "favicon": "./src/assets/temp_icon.png"
    }
  }
}
