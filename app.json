{
    "expo": {
        "name": "Recipes",
        "slug": "Recipes",
        "version": "1.0.0",
        "orientation": "default",
        "icon": "./app/assets/temp_icon.png",
        "splash": {
            "image": "./app/assets/temp_icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#000000"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "softwareKeyboardLayoutMode": "pan",
            "adaptiveIcon": {
                "foregroundImage": "./app/assets/adaptive-icon.png",
                "backgroundColor": "#000000"
            }
        },
        "web": {
            "favicon": "./app/assets/favicon.png"
        }
    }
}
